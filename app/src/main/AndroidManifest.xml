<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="type.proy.com.inventarioandroid" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".actividades.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".actividades.LoginActivity"
            android:label="@string/title_activity_login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".actividades.AbmSoporteActivity"
            android:label="@string/title_activity_abm_soporte" >
            <intent-filter>
                <action android:name="android.intent.action.ADMINISTRAR_SOPORTE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".dom.SoporteActivity"
            android:label="@string/title_activity_soporte" >
            <intent-filter>
                <action android:name="android.intent.action.VER_SOPORTE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".actividades.RegistrarActivity"
            android:label="@string/title_activity_registrar" >
            <intent-filter>
                <action android:name="android.intent.action.REGISTRAR_DATOS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
